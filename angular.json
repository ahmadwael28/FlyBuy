{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Fly-Buy": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/Fly-Buy",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              //"src/styles.css"
              // "src/assets/vendors/bootstrap/bootstrap.min.css",           
              // "src/assets/vendors/fontawesome/css/all.min.css",         
              // "src/assets/vendors/themify-icons/themify-icons.css",    
              // "src/assets/vendors/nice-select/nice-select.css",       
              // "src/assets/vendors/owl-carousel/owl.theme.default.min.css",
              // "src/assets/vendors/owl-carousel/owl.carousel.min.css"  
            ],
            "scripts": [
              // "src/assets/vendors/jquery/jquery-3.2.1.min.js",
              // "src/assets/vendors/bootstrap/bootstrap.bundle.min.js",
              // "src/assets/vendors/skrollr.min.js",    
              // "src/assets/vendors/owl-carousel/owl.carousel.min.js",
              // "src/assets/vendors/nice-select/jquery.nice-select.min.js",
              // "src/assets/vendors/jquery.ajaxchimp.min.js",
              // "src/assets/vendors/mail-script.js",
              // "src/assets/js/main.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "Fly-Buy:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "Fly-Buy:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "Fly-Buy:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "Fly-Buy:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "Fly-Buy:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "Fly-Buy"
}
